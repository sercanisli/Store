@model IEnumerable<Category>
<div class="mt-3">
    <h6 class="lead">
        <i class="fa fa-solid fa-bars text-secondary"></i>
        Cateegories
    </h6>
</div>
<div class="my-4">
    <ul class="list-group">
        @foreach (var category in Model)
        {
            <li class="list-group-item">
                <a asp-action="Index" asp-route-categorId="@category.Id">
                    @category.CategoryName
                </a>
            </li>
        }
    </ul>
</div>